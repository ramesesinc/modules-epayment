<schema adapter="gdx">
	<element tablename="payment">
		<field name="objid" primary="true"/> 
		<field name="partnerid" required="true"/> 
		<field name="partnercode" required="true"/> 
		<field name="paymentrefid" required="true"/> 
		<field name="orgcode" required="true"/> 
		<field name="txntype" required="true"/> 
		<field name="txndate" required="true"/> 
		<field name="traceid" required="true"/> 
		<field name="tracedate" required="true"/> 
		<field name="amount" required="true"/> 
		<field name="receiptid" /> 
		<field name="receiptno" /> 
		<field name="receiptdate" /> 
		<complex name="info" serializer="default" required="true" /> 

		<complex name="partner" ref="payment_partner" jointype="many-to-one" required="false">
			<key field="partnercode" target="objid"/> 
		</complex> 
	</element> 
</schema>

		