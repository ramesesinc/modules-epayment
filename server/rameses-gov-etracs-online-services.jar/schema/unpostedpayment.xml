<schema adapter="eor">
	<element tablename="unpostedpayment">
		<field name="objid" primary="true" prefix="PAY"/>
		<field name="txndate" caption="TxnDate"/> 
		<field name="txntype" caption="TxnType"/> 
		<field name="txntypename" caption="TxnTypeName"/> 
		<field name="paymentrefid" caption="PaymentRefId" searchable="true" indexed="true"/> 
		<field name="amount" caption="Amount"/> 
		<field name="orgcode" caption="OrgCode"/> 
		<field name="partnerid" caption="PartnerId" indexed="true"/> 
		<field name="traceid" indexed="true"/> 
		<field name="tracedate" required="true"/> 
		<field name="refno" caption="RefNo" searchable="true" indexed="true"/> 
		<field name="origin" caption="Origin"/> 
		<complex name="paymentorder" serializer="default" required="true"/>

		<field name="errmsg" caption="ErrMsg"/> 
		<complex name="errdetail" serializer="default" required="false"/>
	</element>
</schema>	

