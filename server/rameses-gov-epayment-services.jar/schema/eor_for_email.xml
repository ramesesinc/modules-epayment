<schema>
	<element tablename="eor_for_email">
		<field name="objid" primary="true" visible="true" caption="EOR No"/>
		<field name="txndate" type="timestamp"/>
		<field name="email" />
		<field name="mobileno" />
		<field name="state" type="integer"/>
		<field name="dtsent" type="timestamp"/>
		<field name="errmsg" />

		<complex name="eor" ref="eor" jointype="many-to-one" includefields="receiptno">
			<key field="objid" target="objid" />
		</complex>	
	</element>
</schema>